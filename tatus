": "String", "max_value": null, "min_value": null, "widget_name": "", "questionnaire_text": "", "calculation": "function patientAge(dob) {\r\n    var dateParts = dob.split(\"-\");\r\n    var day = parseInt(dateParts[2]);\r\n    var month = parseInt(dateParts[1]);\r\n    var year = parseInt(dateParts[0]);\r\n    var birthday = new Date(year,month,day);\r\n    var today = new Date();\r\n    var age =  today - birthday;\r\n    age  = age/ ( 1000.0 * 3600.0 * 24.0 * 365.0) ; \r\n    \r\n    return age;\r\n}\r\n\r\n\r\nfunction getscore(context,patient) {\r\n\r\nvar score = 0;\r\n\r\n\r\nfunction getLDL(context) {\r\n   var L;\r\n   try {\r\n      L = parseFloat(context.CDE00019); \r\n      if ( isNaN(L)) {\r\n        \r\n        throw new Error(\"untreated not filled out\");\r\n      }\r\n      return L;\r\n   }\r\n   catch (err) {\r\n   \r\n      try {\r\n        // try treated value \r\n         L = parseFloat(context.LDLCholesterolAdjTreatment);\r\n         if (! isNaN(L) ){\r\n            return L;\r\n         }\r\n         else {\r\n         \r\n          throw new Error(\"no LDL\");\r\n         }\r\n         \r\n      }\r\n      catch (err2) {\r\n         alert(\"no LDL recorded - score not calculated\");\r\n         throw new Error(\"no LDL\");\r\n      }\r\n    }\r\n}   \r\n\r\n\r\nvar isAdult = patientAge(patient.date_of_birth) >= 18;\r\nvar TENDON_XANTHOMA = context.CDE00001;\r\nvar ARCUS_CORNEALIS = context.CDE00002;\r\nvar FAM_HIST_PREM_CVD = context.CDE00004;\r\nvar FAM_HIST_HYPERCHOL = context.CDE00003;\r\nvar FAM_HIST_TENDON = context.FHFamHistTendonXanthoma;\r\nvar FAM_HIST_ARCUS_CORNEALIS = context.FHFamHistArcusCornealis;\r\nvar PERS_HIST_PREM_CVD = context.CDE00011;\r\nvar PREM_CVD_YES_FIRST_DEGREE_RELATIVE =  \"fh2_y\";\r\nvar PREM_CVD_NO = \"fh2_n\";\r\nvar PREM_CVD_UNKNOWN = \"fh2_u\";\r\nvar FAM_HIST_HYPERCHOL_FIRST_DEGREE_RELATIVE = \"y_firstdegrel\";\r\nvar FAM_HIST_HYPERCHOL_CHILD = \"y_childunder18\";\r\n\r\n\r\nif (isAdult) {\r\n      // CDE00004 \r\n      //add 1 to score if \"Family History of Premature CVD( Men<55, Women<60)\" has been answered with \"Yes, first-degree relative\"\r\n      if ( FAM_HIST_PREM_CVD  == PREM_CVD_YES_FIRST_DEGREE_RELATIVE) {\r\n         score += 1;\r\n      }\r\n      //add 1 to score if \"Family History of Hypercholesterolaemia\" has been answered with \"Yes, first-degree relative\" AND\r\n      //\"Family History of Premature CVD( Men<55, Women<60)\" has been answered with \"No\" or \"Unknown\"\r\n      if ( FAM_HIST_HYPERCHOL == FAM_HIST_HYPERCHOL_FIRST_DEGREE_RELATIVE && ( ( FAM_HIST_PREM_CVD == PREM_CVD_NO) || ( FAM_HIST_PREM_CVD == PREM_CVD_UNKNOWN ) ))  {\r\n       score += 1;\r\n      } \r\n\r\n      //add 2 to score if \"Family History of Hypercholesterolaemia\"has been answered with \"Yes, child aged < 18\"\r\n      if ( FAM_HIST_HYPERCHOL == FAM_HIST_HYPERCHOL_CHILD ) {\r\n        score += 2;\r\n      }\r\n\r\n      //add 2 to score if \"Family History of Tendon xanthoma\" has been answered with \"Yes, first-degree relative\" and\r\n      //\"Family History of Hypercholesterolaemia\" has been answered with \"No\", \"Yes, first-degree relative\" or \"Unknown\"\r\n      if ( (FAM_HIST_TENDON == \"y_firstdegrel\")  && (( FAM_HIST_HYPERCHOL == \"fh_n\")  || ( FAM_HIST_HYPERCHOL == \"fh_u\") || (FAM_HIST_HYPERCHOL == \"y_firstdegrel\")   )) {\r\n        score += 2;\r\n      } \r\n \r\n      //add 2 to score if \"Family History of Arcus cornealis prior to age\r\n      //45 years\" has been answered with \"Yes, first-degree relative\"\r\n      //and \"Family History of Hypercholesterolaemia\" has been answered with \"No\", \"Yes first-degree relative\" or \"Unknown\" and\r\n      //\"Family History of Tendon xanthoma\" has been answered with \"No\" or \"Unknown\"\r\n\r\n      if ( FAM_HIST_ARCUS_CORNEALIS == \"fh2_y\" &&\r\n         ( ((FAM_HIST_HYPERCHOL == \"fh_n\") || (FAM_HIST_HYPERCHOL == \"y_firstdegrel\") || (FAM_HIST_HYPERCHOL == \"fh_u\")) && ((FAM_HIST_TENDON == \"fh2_n\") || (FAM_HIST_TENDON == \"fh2_u\")))) {\r\n        score += 2;\r\n      } \r\n      \r\n      //add 2 to score if \"Personal History of Premature CVD (Men <55, Women <60)\" has been answered with \"Yes, coronary heart disease\"\r\n\r\n      if ( PERS_HIST_PREM_CVD == \"fhpremcvd_yes_corheartdisease\") {\r\n        score += 2;\r\n      }\r\n     \r\n      //add 1 to score  if \"Personal History of Premature CVD (Men <55, Women <60)\" has been answered with \"Yes, cerebral or peripheral vascular disease\"\r\n      if ( PERS_HIST_PREM_CVD == \"fhpremcvd_yes_cerebralorpvd\") {\r\n        score += 1;\r\n      } \r\n      \r\n      //add 6 to score if \"Tendon xanthoma\" has been answered with \"Yes\"\r\n      if ( TENDON_XANTHOMA == \"y\" ) {\r\n       score += 6;\r\n      }\r\n \r\n      //add 4 to score if \"Arcus cornealis prior to age 45 years\" has been answered with \"Yes\"\r\n      if ( ARCUS_CORNEALIS == \"y\" ) {\r\n       score += 4;\r\n      }\r\n\r\n\r\n      var L = getLDL(context);\r\n      if ( ( 4.0 <= L ) && ( L <= 4.9 )) {\r\n            score += 1;\r\n      }\r\n      //add 3 to score if 5.0 <= L <= 6.4\r\n      if ( ( 5.0 <= L ) && ( L <= 6.4 )) {\r\n         score += 3;\r\n      }\r\n      //add 5 to score if 6.5 <= L <= 8.4\r\n      if ( (6.5 <= L ) && ( L <= 8.4)) {\r\n         score += 5;\r\n      }\r\n         \r\n     //add 8 to score if L >= 8.5\r\n\r\n      if ( L >= 8.5) {\r\n         score += 8;\r\n      }\r\n\r\n      return score;\r\n}\r\n \r\n \r\nelse { \r\n    // child\r\n    \r\n    return \"\";\r\n}\r\n\r\n}\r\n\r\n \r\n\r\n\r\n\r\n\r\nfunction catchild(context) {\r\n    var L = getLDL(context);\r\n    \r\n    function anyrel(context) {\r\n      return ( (context.CDE00003 == \"y_firstdegrel\") || ( context.CDE00004 == \"fh2_y\") || ( context.FHFamHistTendonXanthoma == \"fh2_y\") || ( context.FHFamHistArcusCornealis == \"fh2_y\" ) );\r\n    }\r\n     \r\n    if (context.CDE00025 == \"gen_yes\") {\r\n        return \"Definite\";\r\n     }\r\n     //Definite if DNA Analysis is Yes\r\n   //other wise\r\n   else if (L > 5.0) {\r\n     return \"Highly Probable\";\r\n   }\r\n   else if ( (L > 4.0) && anyrel(context)) {\r\n     return \"Highly Probable\";\r\n   }\r\n   \r\n   else if ( (L > 3.3) && ( L <= 4.0) && anyrel(context) ) {\r\n      return \"Possible\";\r\n   }\r\n   else {\r\n     return \"Unlikely\";\r\n   }\r\n   \r\n}\r\n\r\nfunction catadult(score) {\r\n \r\n    if ((0 <= score) && (score <= 2)) {\r\n        return \"Unlikely\";\r\n    }\r\n    \r\n    if ((3 <= score) && (score <= 5)) {\r\n        return \"Possible\";\r\n    }\r\n    \r\n    if ((6 <= score) && (score <= 8)) {\r\n       return \"Probable\";\r\n    }\r\n    \r\n    return \"Definite\";\r\n}\r\n\r\nfunction categorise(context,patient) {\r\n  var isAdult = patientAge(patient.date_of_birth) >= 18.0;\r\n  \r\n  if (isAdult) {\r\n     return catadult(getscore(context,patient));\r\n  }\r\n  \r\n  else {\r\n     return catchild(context);\r\n  }\r\n  \r\n}\r\n\r\n\r\n\r\n\r\n\r\n//context.result = getscore(context,patient);\r\n\r\ncontext.result = categorise(context, patient);", "max_length": null, "pattern": "", "instructions": "", "is_required": false, "desc": ""}}, {"pk": "CDE00025", "model": "rdrf.commondataelement", "fields": {"pv_group": "PVG-FH_GENETIC_VARIANT", "allow_multiple": false, "name": "Has this patient had an FH genetic variant detected?", "datatype": "String", "max_value": null, "min_value": null, "widget_name": "", "questionnaire_text": "", "calculation": "", "max_length": null, "pattern": "", "instructions": "", "is_required": true, "desc": ""}}, {"pk": "CDE00026", "model": "rdrf.commondataelement", "fields": {"pv_group": "PVG-FH-IndexOrRelative", "allow_multiple": false, "name": "Index or Relative", "datatype": "range", "max_value": null, "min_value": null, "widget_name": "", "questionnaire_text": "", "calculation": "", "max_length": null, "pattern": "", "instructions": "Index or relative", "is_r